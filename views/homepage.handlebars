<div class="container-fluid text-center">
  <div class="row row-cols-2">
    {{! groups column }}
    <div class="col">
      <h1>Groups</h1>
      <div class="row row-cols-2">
        {{#each groups as |group|}}
          <div class="col">
            <p>{{group.title}}</p>
          </div>
        {{/each}}
      </div>
    </div>
    {{! list column }}
    <div class="col">
      <h1>Lists</h1>
      <div class="row row-cols-2">
        <div class="col">column</div>
        <div class="col">column</div>
        <div class="col">column</div>
        <div class="col">column</div>
        <div class="col">column</div>
        <div class="col">column</div>
      </div>
    </div>
  </div>
</div>